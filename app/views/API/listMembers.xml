<?xml version='1.0' encoding='utf-8'?>
<members>
	#{list members, as:'member'}
	<member>
		<fullname>${member.fullName}</fullname>
		<activity>${member.activity?.escapeXml()}</activity>
		<company>${member.company}</company>
		<url>${member.url}</url>
		<personalUrl>${member.personalUrl}</personalUrl>
		<description>${member.description?.escapeXml()}</description>
		<photoUrl>${member.photoUrl.escapeXml()}</photoUrl>
	</member>
	#{/list}
</members>